
<main class="main-content">
    {{#if results}}
      {{#each results}}
        <a href="/{{this.id}}" target="_self" class="item-link">
          <div class="item">
            <h2 class="title">{{this.title}}</h2>
            <img class="image" src="{{this.image}}" width="200" height="150" alt="its replacing the image with this text"/>
            <p class="description">{{this.description}}</p>
            <h2 class="price">${{this.price}}</h2>
          </div>
        </a>
      {{/each}}
    {{else}}
      <h2 id="not-found">No Product Found!</h2>
    {{/if}}
  </main>
   <link rel="stylesheet" href="/public/css/searchresults.css">